        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"method\": \"{{request.method}}\", \"path\": \"{{request.path}}\", \"url\": \"{{request.url}}\", \"receivedName\": \"{{request.body.name}}\", \"receivedEmail\": \"{{request.body.email}}\"}"
      }
    }
  },
  {
    "name": "28. Templated Response - Nested Body Access",
    "description": "Access deeply nested JSON body fields",
    "type": "http",
    "enabled": true,
    "http": {
      "matcher": {
        "method": "POST",
        "path": "/api/nested-echo"
      },
      "response": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"userName\": \"{{request.body.user.name}}\", \"userCity\": \"{{request.body.user.address.city}}\", \"userCountry\": \"{{request.body.user.address.country}}\"}"
      }
    }
  },
  {
    "name": "29. Templated Response - String Functions",
    "description": "Use upper and lower string functions",
    "type": "http",
    "enabled": true,
    "http": {
      "matcher": {
        "method": "POST",
        "path": "/api/transform"
      },
      "response": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"original\": \"{{request.body.text}}\", \"uppercase\": \"{{upper request.body.text}}\", \"lowercase\": \"{{lower request.body.text}}\"}"
      }
    }
  },
  {
    "name": "30. Templated Response - Default Values",
    "description": "Use default values when fields are missing",
    "type": "http",
    "enabled": true,
    "http": {
      "matcher": {
        "method": "POST",
        "path": "/api/with-defaults"
      },
      "response": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"name\": \"{{default request.body.name 'Anonymous'}}\", \"role\": \"{{default request.body.role 'guest'}}\"}"
      }
    }
  },
  {
    "name": "31. Templated Response - Random Values",
    "description": "Generate random numbers in response",
    "type": "http",
    "enabled": true,
    "http": {
      "matcher": {
        "method": "GET",
        "path": "/api/random"
      },
      "response": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
