    {
      "type": "http",
      "name": "SSE Combined Timing and Lifecycle",
      "description": "Complex example combining multiple timing and lifecycle features",
      "enabled": true,
      "http": {
        "matcher": {
          "method": "GET",
          "path": "/sse/complex"
        },
        "sse": {
          "events": [
            {
              "type": "init",
              "id": "1",
              "data": "Stream starting",
              "retry": 2000
            },
            {
              "type": "data",
              "id": "2",
              "data": "{\"seq\": 1}",
              "delay": 100
            },
            {
              "type": "data",
              "id": "3",
              "data": "{\"seq\": 2}",
              "delay": 100
            },
            {
              "type": "data",
              "id": "4",
              "data": "{\"seq\": 3}",
              "delay": 100
            },
            {
              "type": "pause",
              "id": "5",
              "data": "Pausing...",
              "delay": 2000
            },
            {
              "type": "data",
              "id": "6",
              "data": "{\"seq\": 4}",
              "delay": 500
            },
            {
              "type": "data",
              "id": "7",
              "data": "{\"seq\": 5}",
              "delay": 500
            },
            {
              "type": "end",
              "id": "8",
              "data": "Stream complete"
            }
          ],
          "timing": {
            "initialDelay": 300
          },
          "resume": {
            "enabled": true,
            "bufferSize": 20,
            "maxAge": 300
          },
          "lifecycle": {
            "keepaliveInterval": 10,
            "timeout": 60,
            "termination": {
              "type": "graceful",
              "finalEvent": {
                "type": "goodbye",
                "data": "{\"message\": \"Connection closed gracefully\"}"
              },
              "closeDelay": 200
            }
          }
        }
      },
      "id": "sse-demo-32"
