{
  "version": "1.0",
  "name": "HTTP SSE Features Demo",
  "description": "Comprehensive demonstration of Server-Sent Events (SSE) mocking features including streaming, generators, and OpenAI-style responses",
  "mocks": [
    {
      "type": "http",
      "name": "SSE Basic Stream - Multiple Events",
      "description": "Basic SSE stream with multiple static events demonstrating fundamental SSE functionality",
      "enabled": true,
      "http": {
        "matcher": {
          "method": "GET",
          "path": "/sse/basic"
        },
        "sse": {
          "events": [
            {
              "data": "Hello, SSE!"
            },
            {
              "data": "This is event 2"
            },
            {
              "data": "This is event 3"
            },
            {
              "data": "Stream complete"
            }
          ],
          "timing": {
            "fixedDelay": 500
          },
          "lifecycle": {}
        }
      },
      "id": "sse-demo-01"
    },
    {
      "type": "http",
      "name": "SSE Event Types - Named Events",
      "description": "Demonstrates different event types/names that clients can listen for with addEventListener",
      "enabled": true,
      "http": {
        "matcher": {
          "method": "GET",
          "path": "/sse/event-types"
        },
        "sse": {
          "events": [
            {
              "type": "connect",
              "data": "{\"status\": \"connected\", \"timestamp\": \"$timestamp\"}"
            },
            {
              "type": "message",
              "data": "{\"content\": \"Regular message\"}"
            },
            {
              "type": "notification",
              "data": "{\"title\": \"Alert\", \"body\": \"Something happened\"}"
            },
            {
              "type": "heartbeat",
              "data": "ping"
            },
            {
              "type": "update",
              "data": "{\"field\": \"score\", \"value\": 100}"
            },
            {
              "type": "disconnect",
              "data": "{\"reason\": \"stream complete\"}"
            }
          ],
          "timing": {
            "fixedDelay": 300
          },
          "lifecycle": {}
        }
      },
      "id": "sse-demo-02"
    },
    {
      "type": "http",
      "name": "SSE Event IDs - Resumable Stream",
      "description": "Events with IDs enabling Last-Event-ID resumption support",
      "enabled": true,
      "http": {
        "matcher": {
          "method": "GET",
          "path": "/sse/with-ids"
        },
        "sse": {
          "events": [
            {
              "id": "evt-001",
              "data": "First event"
            },
            {
              "id": "evt-002",
              "data": "Second event"
            },
            {
              "id": "evt-003",
              "data": "Third event"
            },
            {
              "id": "evt-004",
              "data": "Fourth event"
            },
            {
              "id": "evt-005",
              "data": "Fifth event"
            }
          ],
          "timing": {
            "fixedDelay": 400
          },
          "resume": {
            "enabled": true,
            "bufferSize": 100,
            "maxAge": 3600
          },
          "lifecycle": {}
        }
      },
      "id": "sse-demo-03"
    },
    {
      "type": "http",
      "name": "SSE Retry Interval - Client Reconnection",
      "description": "Events with retry field to configure client reconnection intervals",
      "enabled": true,
      "http": {
        "matcher": {
          "method": "GET",
          "path": "/sse/retry-interval"
        },
        "sse": {
          "events": [
            {
              "retry": 5000,
              "data": "First event - retry set to 5 seconds"
            },
            {
              "data": "Second event - uses previous retry"
            },
            {
              "retry": 2000,
              "data": "Third event - retry changed to 2 seconds"
            },
            {
              "data": "Fourth event - uses 2 second retry"
            }
          ],
          "timing": {
            "fixedDelay": 600
          },
          "lifecycle": {}
        }
      },
      "id": "sse-demo-04"
    },
