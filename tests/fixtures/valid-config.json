      "name": "Create User",
      "priority": 10,
      "enabled": true,
      "matcher": {
        "method": "POST",
        "path": "/api/users",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "statusCode": 201,
        "headers": {
