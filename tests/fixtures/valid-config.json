      "name": "Get User by ID",
      "priority": 20,
      "enabled": true,
      "matcher": {
        "method": "GET",
        "path": "/api/users/123",
        "queryParams": {
          "include": "profile"
        }
      },
      "response": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"id\": \"123\", \"name\": \"Alice\", \"profile\": {\"email\": \"alice@example.com\"}}",
        "delayMs": 100
      },
      "createdAt": "2025-11-17T10:00:00Z",
      "updatedAt": "2025-11-17T10:00:00Z"
    },
